import heapq
class Solution(object):
    def kClosest(self, points, k):
        heap = []
        for (x, y) in points:
            dist = -(x**2 + y**2)
            heapq.heappush(heap, (dist, x, y))
            if len(heap) > k:
                heapq.heappop(heap)
        return [(x, y) for (_, x, y) in heap]